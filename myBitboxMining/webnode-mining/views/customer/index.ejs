<%- include('../share_customer/header'); %>
<%- include('../share_customer/navbar'); %>
<div class="content">
    <div id="notification"></div>
    <div class="container-fluid">
        <div class="row" id="eth-exchange">
            
        </div>
        <div class="row">
            <div class="col-md-12">
                <div class="card card-chart" id="eth-chart">
                    
                </div>
            </div>
        </div>

        <div id="list-product">
        </div>

        <div class="row">
            <div class="col-lg-6 col-md-12">
                <div id="table-product-customer">
                    
                </div>
            </div>
            <div class="col-lg-6 col-md-12">
                <div class="card" id="table-history-customer">
                </div>
            </div>
        </div>
    </div>
</div>
<script>
    var pageContext = {
        "page" : '<%- menu_active%>',
        "totalHs" : '<%- totalHs%>',
        "unpaidBalance" : '<%- unpaidBalance%>'
    }
</script>
<script type="text/javascript">
    var baseUrl = "https://widgets.cryptocompare.com/";
    var scripts = document.getElementById('eth-chart');
    (function (){
        var appName = encodeURIComponent(window.location.hostname);
        if(appName==""){appName="local";}
        var s = document.createElement("script");
        s.type = "text/javascript";
        s.async = true;
        var theUrl = baseUrl+'serve/v3/coin/chart?fsym=ETH&tsyms=USD,EUR,CNY,GBP';
        s.src = theUrl + ( theUrl.indexOf("?") >= 0 ? "&" : "?") + "app=" + appName;
        scripts.appendChild(s);
    })();
</script>
