<%- include('../../share_customer/header'); %>
<%- include('../../share_customer/navbar'); %>
<div class="content" >
    <div class="col-lg-12 col-md-12">
        <div class="row">
            <a href="/withdraw-eth" class="btn btn-round btn-success">
                <i class="material-icons">add</i>New request withdraw ETH<div class="ripple-container"></div>
            </a>
        </div>
        <div class="card">
            <div class="card-header card-header-warning">
                <h4 class="card-title">Withdraw ETH</h4>
            </div>
            <% if (obj_data.dataResult.length > 0) {%>
            <div class="card-body table-responsive">
                <table class="table table-hover text-center">
                    <thead class="text-warning">
                        <tr>
                            <th> <%= showMessage('RC_ID') %> </th>
                            <th> <%= showMessage('MAINTAIN_ETH') %> </th>
                            <th> <%= showMessage('RC_DESCRIPTION') %> </th>
                            <th> <%= showMessage('MAINTAIN_FEE_STATUS') %> </th>
                            <th> <%= showMessage('RC_ACTION') %> </th>
                        </tr>
                    </thead>
                    <tbody>
                        <% for(var i = 0; i < obj_data.dataResult.length; i++) { var row = obj_data.dataResult[i]; %>
                            <tr>
                                <td> <%- i+1 %> </td>
                                <td> <%- row.total_eth %> </td>
                                <td> <%- row.description %> </td>
                                <td> 
                                    <% if(row.status === 0) {%> 
                                        New
                                    <% } else if(row.status === 1) {%>
                                        Create
                                    <% } else if(row.status === 2) {%>
                                        Approve
                                    <% } else { %>
                                        Done
                                    <% } %>
                                </td>
                                <td>
                                    <% if(row.status < 2) {%>
                                        <button type="button" rel="tooltip" title="" class="btn btn-danger btn-link btn-sm" data-original-title="Remove" >
                                            <i class="material-icons">close</i>
                                        </button>
                                    <%}%>
                                </td>
                            </tr>
                        <% } %>
                    </tbody>
                </table>
            </div>
            <% } else {%>
                <div class="card-body">
                    <div class="form-group">
                        <% if(obj_data.err !== '') {%>
                            <span class="note"> <%- obj_data.err %> </span>
                        <% } else {%>
                            <span></span>
                        <% } %>
                    </div>
                </div>
            <% } %> 
        </div>
    </div>
</div>
<script>
    var pageContext = {
        "page" : "<%- menu_active %>"
    }
</script>

<%- include('../../share_customer/footer'); %>