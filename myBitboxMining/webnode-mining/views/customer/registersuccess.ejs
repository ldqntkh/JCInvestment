<%- include('../share_customer/login/header', {"title" : title}); %>
    <div class="content">
        <div class="container-fluid register-account-container">
            <div class="row justify-content-md-center">
                <div class="col-md-6 register-account-form">
                    <div class="card">
                        <div class="card-header card-header-primary">
                            <h4 class="card-title">Register successfully</h4>
                        </div>
                        <div class="card-body">
                            <div class="row">
                                <div class="col-md-12">
                                    <%= typeof message !== 'undefined' ? message : ''%>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
</div>
</body>